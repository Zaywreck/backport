{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "index.js"
    },
    {
      "src": "/api/.*",
      "methods": ["OPTIONS"],
      "dest": "index.js"
    }
  ]
}
